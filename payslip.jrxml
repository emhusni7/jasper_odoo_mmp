	<queryString>
		<![CDATA[SELECT 
	sl.id,
	hsr."sequence",
	he."name" employee,
	run.month,
	sl.number,
	hd.name department,
	hpl.name slip_name,
	hpl.total,
	hsr.code
	
	
from hr_payslip_run run 
	inner join hr_payslip sl on sl.payslip_run_id  = run.id 
	inner join hr_employee he on he.id = sl.employee_id
	inner join hr_department hd on he.department_id = hd.id 
	inner join hr_payslip_line hpl on hpl.slip_id = sl.id 
	inner join hr_salary_rule hsr on hsr.id = hpl.salary_rule_id 
where run.id =  $P{id} 
order by sl.id, hsr."sequence"]]>
	</queryString>
